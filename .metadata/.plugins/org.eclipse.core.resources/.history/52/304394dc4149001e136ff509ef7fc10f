package com.abc;
public class App 
{
    public static void main( String[] args )
    {
    	int a=101;
    	int rev=0,rem;
    	int num=a;
    	while(a>0) {
    		rem=a%10;
    		rev=rev*10+rem;
    		a=a/10;
    	}
    	System.out.println(rev);
    	if(rev==num) {
    		System.out.println("palinderome");
    	}else {
    		System.out.println("not a palindrome");
    	}
    }
}
